{
  "extends": "tslint-config-standard",
  "rules": {
    "no-floating-promises": false,
    "no-unused-variable": true,
    "semicolon": true,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "trailing-comma": true,
    "no-console": [
      true,
      "log"
    ]
  }
}